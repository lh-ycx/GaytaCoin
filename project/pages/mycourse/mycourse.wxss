.icon{
  height: 40rpx;
  width: 40rpx;
  margin-left: 100rpx;
  margin-right: 30rpx;
}

.my-course-item{
	display: flex;
	padding: 20rpx;
	border-bottom: 1rpx solid #ddd;
}

.course-icon{
	width: 200rpx;
	height: 156rpx;
}
.course-icon image{
	width: 200rpx;
	height: 156rpx;
}


.my-course-text{
	flex: 1;
	margin-left: 20rpx;
	width: 100%;
}
.my-course-title{
	color: #333;
	width: 100%;
	font-size: 34rpx;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.my-course-text text{
	display: block;
	color: #888;
}